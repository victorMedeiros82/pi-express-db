<h1>{{title}}</h1>
<table class="table table-striped">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Matrícula</th>
      <th scope="col">Nome</th>
      <th scope="col">Data de Nascimento</th>
      <th scope="col">Endereço de Email</th>
    </tr>
  </thead>
  {{#each alunos}}
  <tbody>
    <tr>
      <th scope="row">1</th>
      <td>{{matricula}}</td>
      <td>{{nome}}</td>
      <td>{{data_nascimento}}</td>
      <td>{{email}}</td>
    </tbody>
{{/each}}
</table>
<button type="button" class="btn btn-outline-primary btn-lg">Novo Aluno <i class="bi bi-person-add"></i></button>
